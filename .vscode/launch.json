  "version": "0.2.0",
  "configurations": [
      {
          "name": "Python: Remote Attach to Cat",
          "type": "python",
          "request": "attach",
          "connect": {
              "host": "localhost",
              "port": 5678
          },
          "pathMappings": [
              {
                  "localRoot": "${workspaceFolder}/cat/plugins",
                  "remoteRoot": "/app/cat/plugins"
              }
          ],
          "justMyCode": true
      }
  ]
}